{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/vue-netcore2spa.web/vue-netcore2spa.csproj"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "run",
      "command": "dotnet",
      "type": "process",
      "args": [
        "run",
        "${workspaceFolder}/vue-netcore2spa.web/vue-netcore2spa.csproj"
      ],
      "problemMatcher": "$msCompile",
      "dependsOn": "build"
    }
  ]
}
